# src/wl/linux driver config file

#include $(WLCFGDIR)/wlconfig_lx_wl_apdef
#include $(WLCFGDIR)/wlconfig_lx_wl_stadef

ARMV7L=1
STBLINUX=1

# define WET only for test builds, for mfgtest builds only.
ifdef WLTEST
	WET=1
endif

# WLLXIW=0 to get around cfg80211 support being broken in the older drivers,
# but for 2015 when STB platfomrs switch to wpa_supplicant this should be changed.
WLLXIW=0
